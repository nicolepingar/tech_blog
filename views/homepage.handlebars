{{#each posts as |post|}}
<div class="card text-center">
    <div class="card-body">
        <h5 class="card-title">{{title}}</h5>
        <p class="card-text">{{contents}}</p>
        <a href="#" class="btn btn-primary">Comment</a>
    </div>
    <div class="card-footer text-muted">
        Made by: {{users.username}} on {{format_date date}}.
    </div>
</div>
{{/each}}